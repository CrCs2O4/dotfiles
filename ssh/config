	# RemoteForward 52698 localhost:52698

Host *
	StrictHostKeyChecking ask
	VerifyHostKeyDNS ask
	NoHostAuthenticationForLocalhost yes
	IdentityFile ~/.ssh/id_rsa
	ControlMaster auto
	ControlPath /tmp/ssh-%r@%h:%p.socket
