[user]
	name = Chromium
	email = crcs2o4@gmail.com
[shell]
	# Default SSH username.
	username = chromium
[bintray]
	# Bintray username.
	username = chromium
[github]
	# GitHub username for command-line tools.
	user = crcs2o4
[alias]
	# Push the current branch upstream to crcs2o4 using the same
	# branch name for the remote branch.
	um = !(git remote -v | grep -q crcs2o4 || hub fork) \
	    && git push --set-upstream crcs2o4 $(git current-branch)
